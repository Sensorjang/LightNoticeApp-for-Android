<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="AppTheme1" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:alertDialogTheme">@style/DialogTheme1</item>
        <item name="android:colorPrimary">@color/AppTheme1colorPrimary</item>
        <item name="android:colorAccent">@color/AppTheme1colorPrimary</item>
        <item name="colorPrimary">@color/AppTheme1colorPrimary</item>
        <item name="colorAccent">@color/AppTheme1colorPrimary</item>
        <item name="android:actionModeBackground">@color/AppTheme1colorPrimary</item>
        <item name="materialAlertDialogTheme">@style/DialogTheme1</item>
        <item name="android:colorPrimaryDark">#ff0096ff</item>
        <item name="android:toolbarStyle">@style/ActionBarTheme1</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="android:statusBarColor">@color/AppTheme1colorPrimary</item>
        <item name="android:textColorPrimary">#FF000000</item>
        <item name="android:navigationBarColor">@color/AppTheme1colorPrimary</item>
    </style>


    <style name="ActionBarTheme1" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="colorPrimary">@color/AppTheme1colorPrimary</item>
        <item name="colorAccent">@color/AppTheme1colorPrimary</item>
        <item name="colorControlNormal">#ffffffff</item>
        <item name="android:actionModeBackground">@color/AppTheme1colorPrimary</item>
        <item name="android:popupBackground">#ffffffff</item>
        <item name="android:colorBackground">#ffffffff</item>
    </style>

    <declare-styleable name="MyRichTextEditor">
        <attr name="editText_Background" format="color|reference"/>
        <attr name="editText_Color" format="color"/>
        <attr name="editText_Font" format="string"/>
        <attr name="editText_Style" format="reference|string"/>
        <attr name="editText_Size" format="dimension|integer"/>
    </declare-styleable>





